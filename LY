<button onclick="getCurTime()" type="button">获取当前时间点</button>
<button onclick="setCurTime()" type="button">设置时间位置为5秒</button>
<script>
var x = document.getElementById("myVideo");
function getCurTime(){ 
	alert(x.currentTime);
} 
function setCurTime(){ 
	x.currentTime=x.currentTime - 5000;

} 
</script> 

